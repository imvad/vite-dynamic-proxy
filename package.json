{
  "name": "vite-dynamic-proxy",
  "version": "1.0.0",
  "description": "A Vite plugin that enables dynamic proxy configuration at runtime, allowing flexible and configurable proxy settings for development servers",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "proxy",
    "development",
    "dev-server",
    "middleware",
    "dynamic-proxy",
    "runtime-config",
    "typescript",
    "http-proxy",
    "api-proxy",
    "reverse-proxy"
  ],
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitest/coverage-v8": "^1.1.3",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.1.3"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  },
  "engines": {
    "node": ">=14.6.0"
  },
  "author": "Vadim Alakhverdov",
  "license": "ISC"
}
